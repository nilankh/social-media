        <h1 id="s2">
            Sociobook / <%= title %>
        </h1>

        <section id="feed-posts">
            <h4>Posts</h4>
            <form action="/posts/create" id="new-post-form" method="POST">
                <textarea name="content" cols="30" rows="4" placeholder="Type here...."></textarea>
                <input type="submit" value="Post">
            </form>
        </section>
        <div id="posts-list-container">
            <ul>
                <% for(post of posts) { %>
                    <li>
                        <p>
                            <%= post.content %>
                        <br>
                        <small>
                            <%= post.user.name %>
                        </small>
                        </p>
                        
                    </li>
               <% } %>
            </ul>
        </div>
